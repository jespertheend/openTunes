{
	"extends": "./.denoTypes/tsconfig.json",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"target": "esnext",
		"checkJs": true,
		"module": "esnext",
		"useDefineForClassFields": true,
		"strict": true,
		"typeRoots": [".denoTypes/@types"],
		"noEmit": true
	},
	"include": [
		"client/**/*.js",
		"server/**/*.ts",
		"scripts/**/*.js",
		"test/**/*.js"
	],
	"files": [
		"main.js"
	]
}
